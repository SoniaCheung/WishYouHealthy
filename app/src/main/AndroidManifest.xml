<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wyh.wishyouhealthy" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Schedule"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".Setting"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".Appset"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".Notification"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".HealthTips"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".Feedback"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".AppsetAA"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".AppsetHelp"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".AppsetTOU"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".Alarm"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".DemoActivity"
            android:label="Demo Activity Screen"
            android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="in.wptrafficanalyzer.servicealarmdemo.demoactivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".Ch_appoint"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".Can_appoint"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".Mk_appoint"
            android:screenOrientation="portrait">
        </activity>

    </application>

</manifest>
